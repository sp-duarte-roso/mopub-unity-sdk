<MoPubStyles>

    <Group
            Style="MPTopGroup"
            Width="100%"
            Height="100%"
            Spacing="10"
            Orientation="Vertical"
            Alignment="Top"
            BackgroundColor="#ffffff"
            BackgroundMaterial="UI-Fast-Default"
    />
    <Label
            Style="MPLabel"
            Width="100%"
            Margin="30,0,10,0"
            Alignment="Left"
            EnableWordWrapping="false"
    />
    <Label
            Style="MPHeader"
            BasedOn="MPLabel"
            Width="100%"
            TextAlignment="Center"
            FontSize="48"
            Offset="0"
            Margin="-3"
            Height="120"
    />
    <Region
            Style="MPHeaderDelimiter"
            Height="2"
            Offset="0,23"
            BackgroundColor="#cfcfcf"
            BackgroundMaterial="UI-Fast-Default" />
    <Region
            Style="MPHeaderShadow"
            Height="3"
            Offset="0,8"
            BackgroundColor="#aaaaaa"
            BackgroundMaterial="UI-Fast-Default" />
    <Region
            Style="MPSectionTitle"
            Height="80"
            BackgroundColor="#286295EF"
            BackgroundMaterial="UI-Fast-Default"
    />
    <Label
            Style="MPSectionTitle"
            BasedOn="MPLabel"
            Width="100%"
            Margin="30"
            FontSize="46"
            FontColor="#242424"
    />
    <ScrollableRegion
            Style="MPScrollableRegion"
            VerticalScrollbarBarIsActive="False"
            Offset=""
            HasInertia="True"
            Elasticity=".15"
            DecelerationRate=".05"
            CanScrollVertically="True"
            CanScrollHorizontally="False"
            MaskContent="True"
    />
    <Region
            Style="MPAdUnitList"
            BackgroundColor="#ffffff"
            BackgroundMaterial="UI-Fast-Default"
    />
    <List
            Style="MPAdUnitList"
            CanSelect="false"
    />
    <ListItem
            Style="MPListItem"
            Height="160"
            BackgroundMaterial="UI-Fast-Default"
            Selected-BackgroundColor="White"
            Highlighted-BackgroundColor="White"
            Pressed-BackgroundColor="White"
    />
    <Label
            Style="MPTitle"
            BasedOn="MPLabel"
            Width="100%"
            Margin="30"
            FontColor="#242424"
            Height="80"
            FontSize="46"
    />
    <Label
            Style="MPSubtitle"
            BasedOn="MPLabel"
            Width="100%"
            Margin="30"
            Height="40"
            FontSize="40"
            FontColor="#aaaaaa"
            Alignment="Left"
            AutoSize="True"
    />
    <Label
            Style="MPAdUnitItemName"
            BasedOn="MPTitle"
            Height="80"
            FontSize="46"
            Width="100%"
            Margin="30,-60"
    />
    <Label
            Style="MPAdUnitItemId"
            BasedOn="MPSubtitle"
            Height="40"
            FontSize="40"
            FontColor="#aaaaaa"
            Width="100%"
            Margin="30,60"
    />
    <Region
            Style="MPDelimiter"
            Height="2"
            Margin="45,0,45,0"
            BackgroundColor="#e3e3e4"
            BackgroundMaterial="UI-Fast-Default"
    />
    <Region
            Style="MPItemDelimeter"
            BasedOn="MPDelimiter"
            Height="2"
            Margin="45,0,45,0"
            BackgroundColor="#e3e3e4"
            Offset="0,80,0,0"
    />
    <InputField
            Style="MPTextEntry"
            Alignment="Left"
            Height="100"
            Width="100%"
            TextAlignment="Left"
            FontSize="40"
            FontColor="#aaaaaa"
            SelectionColor="#6295ef"
            Margin="30,0,30,0"
            TextMargin="0"
            BackgroundMaterial="UI-Fast-Default"
    />
    <Button
            Style="MPBack"
            Height="80"
            Width=""
            FontSize="50"
            Margin="10,10"
            TextMargin="0,0,15,0"
            Alignment="Left"
            BackgroundColor="clear"
            FontColor="#6295ef"
            Highlighted-FontColor="#6295ef"
            Highlighted-BackgroundColor="clear"
            Pressed-BackgroundColor="#6295ef"
            Pressed-FontColor="#ffffff"
            Disabled-FontColor="Gray80"
            Disabled-BackgroundColor="#7d7d7d"
            BackgroundMaterial="UI-Fast-Default"
    />
    <Button
            Style="MPAdAction"
            Height="80"
            Width="200"
            FontSize="40"
            Margin="0"
            TextMargin="-10,0"
            BackgroundColor="#6295ef"
            FontColor="#ffffff"
            Highlighted-FontColor="#ffffff"
            Highlighted-BackgroundColor="#6295ef"
            Pressed-BackgroundColor="#446295ef"
            Pressed-FontColor="#6295ef"
            Disabled-FontColor="#ffffff"
            Disabled-FontStyle="normal"
            Disabled-BackgroundColor="#9c9c9c"
            BackgroundMaterial="UI-Fast-Default"
    />
    <Expander
            Style="MPEventExpander"
            ContentMargin="0,0,60,0"
            Width="100%"
            IsExpanded="false"
            HeaderAlignment="Top"
            BackgroundColor="Clear"
            BackgroundMaterial="UI-Fast-Default"
    />
    <ExpanderHeader
            Style="MPEventHeader"
            Size="100%,100"
    />
    <ExpanderContent
            Style="MPEventContent"
            Size="100%,60"
            Offset="60,90"
            Alignment="Top"
            AutoSizeToContent="True"
            BackgroundColor="Clear"
            BackgroundMaterial="UI-Fast-Default"
    />
    <Group
            Style="MPEventGroup"
            Size="100%,100"
            BasedOn="MPTopGroup"
            Offset="0,-30"
            Orientation="Vertical"
            BackgroundColor="Clear"
            BackgroundMaterial="UI-Fast-Default"
    />
    <Image
            Style="MPEventChevron"
            Height="60"
            Width="60"
            Alignment="Left"
            Sprite="chevron-right"
            Material="UI-Fast-Default"
    />
    <Image
            Style="MPAdUnitChevron"
            Height="60"
            Width="60"
            Alignment="Right"
            Offset="0,0,20"
            Sprite="chevron-right"
            Material="UI-Fast-Default"
    />
    <Label
            Style="MPEventLabel"
            BasedOn="MPTitle"
            EnableWordWrapping="False"
            Width="100%"
            FontColor="#242424"
            Height="80"
            FontSize="46"
            Margin="60"
    />
    <InputField
            Style="MPTextArea"
            BasedOn="MPTextEntry"
            Alignment="Left"
            Height="40"
            FontSize="32"
            FontColor="#aaaaaa"
            SelectionColor="#6295ef"
            TextMargin="0"
            BackgroundMaterial="UI-Fast-Default"
            Width="100%"
            Offset="30,-20"
            Margin="0,0,80,0"
            LineLimit="0"
            LineType="MultiLineNewline"
    />
    <Image
            Style="MPCheckImage"
            Height="60"
            Width="60"
            Offset="0,0,30,0"
            Alignment="Right"
            Sprite="checkmark"
            Material="UI-Fast-Default"
    />

</MoPubStyles>
